{
  "name": "flipboxfactory/patron-salesforce",
  "description": "A Salesforce OAuth2 provider for Patron and Craft CMS",
  "version": "1.1.0",
  "type": "craft-plugin",
  "license": "MIT",
  "keywords": [
    "cms",
    "craftcms",
    "salesforce",
    "force.com",
    "yii2"
  ],
  "require": {
    "stevenmaguire/oauth2-salesforce": "^2.0",
    "flipboxfactory/patron": "^2.0",
    "flipboxfactory/craft-salesforce": "^1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.2",
    "codeception/codeception": "^2.4",
    "vlucas/phpdotenv": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\patron\\salesforce\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\patron\\salesforce\\tests\\": "tests/src/"
    }
  },
  "support": {
    "email": "support@flipboxfactory.com",
    "issues": "https://github.com/flipboxfactory/patron-salesforce/issues?state=open",
    "source": "https://github.com/flipboxfactory/patron-salesforce",
    "docs": "https://github.com/flipboxfactory/patron-salesforce",
    "rss": "https://github.com/flipboxfactory/patron-salesforce/commits/master.atom"
  },
  "extra": {
    "name": "Patron: Salesforce OAuth2 Provider",
    "handle": "patron-salesforce",
    "class": "flipbox\\patron\\salesforce\\Patron",
    "developer": "Flipbox Digital",
    "developerUrl": "https://www.flipboxdigital.com",
    "changelogUrl": "https://raw.githubusercontent.com/flipboxfactory/patron-salesforce/master/CHANGELOG.md",
    "documentationUrl": "https://github.com/flipboxfactory/patron-salesforce/blob/master/README.md",
    "schemaVersion": "1.1.0"
  }
}